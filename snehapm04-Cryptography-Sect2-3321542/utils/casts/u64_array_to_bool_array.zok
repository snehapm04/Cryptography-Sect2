from "EMBED" import u64_to_bits;

def main<N, P>(u64[N] input) -> bool[P] {
    assert(P == 64 * N);

    bool[P] mut res = [false; P];

    for u32 i in 0..N {
        bool[64] bits = u64_to_bits(input[i]);
        for u32 j in 0..64 {
            res[i * 64 + j] = bits[j];
        }
    }

    return res;
}

